@model OriginalFile
@{ Layout = "_Layout"; ViewBag.Title = "All Users";}



<table class="table table-sm table-striped table-bordered m-2">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Avatar</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var r in Model.data)
        {
            <tr>
                <td>@r.first_name</td>
                <td>@r.last_name</td>
                <td>@r.email</td>
                <td>@r.avatar</td>
            </tr>
        }
    </tbody>
</table>
