

@model User
@{ Layout = "_Layout"; ViewBag.Title = "Specific Users";}



<table class="table table-sm table-striped table-bordered m-2">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
            <th>Avatar</th>

        </tr>
    </thead>
    <tbody>


        <tr>
            <td>@Model.first_name</td>
            <td>@Model.last_name</td>
            <td>@Model.email</td>
            <td>@Model.avatar</td>
        </tr>

    </tbody>
</table>
